<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Lambda name="Lambda">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
    </Lambda>
  </Appenders>
  <Loggers>
    <Root level="error">
      <AppenderRef ref="Lambda" />
    </Root>
  </Loggers>
</Configuration>
